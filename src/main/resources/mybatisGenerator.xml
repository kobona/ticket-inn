<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE generatorConfiguration
    PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
    "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">
<generatorConfiguration>
  <context id="DB2Tables" targetRuntime="MyBatis3">

    <!--关闭注释 -->
    <commentGenerator>
      <property name="suppressAllComments" value="true"/>
    </commentGenerator>

    <!--数据库连接信息 -->
    <jdbcConnection driverClass="org.sqlite.JDBC" connectionURL="jdbc:sqlite:/data/ticket_inn.db">
    </jdbcConnection>

    <!--生成的model 包路径 -->
    <javaModelGenerator targetPackage="org.metropolis.dao.model" targetProject="meta">
      <property name="enableSubPackages" value="true"/>
    </javaModelGenerator>

    <!--生成xml mapper文件 路径 -->
    <sqlMapGenerator targetPackage="mapper" targetProject="meta">
      <property name="enableSubPackages" value="true"/>
    </sqlMapGenerator>

    <!-- 生成的Dao接口 的包路径 -->
    <javaClientGenerator type="XMLMAPPER" targetPackage="org.metropolis.dao.mapper" targetProject="meta">
      <property name="enableSubPackages" value="true"/>
    </javaClientGenerator>

    <!--对应数据库表名，多个表，请复制指定 -->
    <table tableName="account" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>
    <table tableName="show" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>
    <table tableName="ticket" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>
    <table tableName="ticket_stamp" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>
    <table tableName="object_slot" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>
    <table tableName="history" enableCountByExample="false" enableUpdateByExample="false" enableDeleteByExample="false" enableSelectByExample="false" selectByExampleQueryId="false"/>


  </context>
</generatorConfiguration>
